{
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "bracketSpacing": true,
    "jsxSingleQuote": false,
    "printWidth": 120,
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "overrides": [
        {
            "files": "*.{js,jsx,ts,tsx}",
            "options": {
                "parser": "babel"
            }
        }
    ]
}